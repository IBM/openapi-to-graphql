{
  "name": "openapi-to-graphql",
  "version": "1.2.0",
  "description": "Generates a GraphQL schema for a given OpenAPI Specification (OAS)",
  "copyright.owner": "IBM Corp.",
  "contributors": [
    "Alan Cha",
    "Erik Wittern"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm/openapi-to-graphql"
  },
  "homepage": "https://github.com/ibm/openapi-to-graphql",
  "keywords": [
    "ibm",
    "strongloop",
    "loopback",
    "oas",
    "openapi specification",
    "graphql",
    "translation",
    "wrap",
    "create",
    "rest",
    "restful",
    "api",
    "apiharmony"
  ],
  "license": "MIT",
  "standard": {
    "ignore": [
      "*.js"
    ]
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "api": "nodemon test/example_api_server.js",
    "dev": "tsc -w",
    "start_dev": "DEBUG=preprocessing,translation,http nodemon test/example_gql_server.js",
    "build": "tsc",
    "guru-load": "node test/evaluation/load_apis_guru.js",
    "guru-test": "DEBUG=preprocessing,translation node test/evaluation/eval_apis_guru.js",
    "test": "npm run build && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@types/request": "^2.48.1",
    "debug": "^4.1.0",
    "deep-equal": "^1.0.1",
    "graphql-type-json": "^0.2.1",
    "json-schema-merge-allof": "^0.6.0",
    "jsonpath-plus": "^0.18.0",
    "oas-validator": "^3.1.0",
    "request": "^2.88.0",
    "swagger2openapi": "^5.1.0"
  },
  "peerDependencies": {
    "graphql": "^0.9.6 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^11.9.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "glob": "^7.1.3",
    "husky": "^2.3.0",
    "isomorphic-git": "^0.51.6",
    "jest": "^24.8.0",
    "js-yaml": "^3.13.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "rimraf": "^2.6.1",
    "simple-statistics": "^7.0.0",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
