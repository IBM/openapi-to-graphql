    "/users": {
      "get": {
        "operationId": "getUsers",
        "description": "Return a list of users.",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer"
            },
            "description": "Limit of the number of users to return."
          }
